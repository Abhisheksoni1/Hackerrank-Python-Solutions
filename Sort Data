# Enter your code here. Read input from STDIN. Print output to STDOUT
m,n=map(int,raw_input().split(" "))
lis=[]
for i in range(m):
    lis.append(map(int,raw_input().split()))
k=int(raw_input())
for i in range(m-1):
    for j in range(m-1):
        if lis[j][k]>lis[j+1][k]:
            temp=lis[j]
            lis[j]=lis[j+1]
            lis[j+1]=temp
for i in range(m):
    for j in range(n):
        print lis[i][j],
    print""
